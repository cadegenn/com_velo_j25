<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>
		<field
			name="id"
			type="hidden"
		/>
		<field
			name="label" type="text"
			label="COM_VELO_LABEL_LABEL" description="COM_VELO_LABEL_DESC"
			size="100" class="inputbox required" default="" required="true"
		/>
		<field
			name="url" type="url"
			label="COM_VELO_URL_LABEL" description="COM_VELO_URL_DESC"
			size="100" class="inputbox" default=""
		/>
		<field
			name="logo" type="media"
			label="COM_VELO_LOGO_LABEL" description="COM_VELO_LOGO_DESC"
			size="100" class="inputbox" default=""
		/>
		<field
			name="favicon" type="media"
			label="COM_VELO_FAVICON_LABEL" description="COM_VELO_FAVICON_DESC"
			size="100" class="inputbox" default=""
		/>
		<field
			name="created_by" type="user"
			label="JGLOBAL_FIELD_CREATED_BY_LABEL" description="JGLOBAL_FIELD_CREATED_BY_DESC"
		/>
		<field
			name="creation_date" type="calendar"
			label="JGLOBAL_CREATED_DATE" description="JGLOBAL_FIELD_CREATED_DESC"
			class="date" default="" size="20"
		/>
		<field
			name="modified_by" type="user"
			label="JGLOBAL_FIELD_MODIFIED_BY_LABEL" description="JGLOBAL_FIELD_MODIFIED_BY_DESC"
			class="readonly" readonly="true" size="20"
		/>
		<field
			name="modification_date" type="calendar"
			label="JGLOBAL_FIELD_MODIFIED_LABEL" description="JGLOBAL_FIELD_MODIFIED_DESC"
			class="readonly" readonly="true" size="20"
		/>
		<field	name="published" type="list" label="JSTATUS"
				description="JFIELD_PUBLISHED_DESC" class="inputbox"
				filter="intval" size="1" default="1"
		>
                    <option value="1">JPUBLISHED</option>
                    <option value="0">JUNPUBLISHED</option>
                    <option value="2">JARCHIVED</option>
                    <option value="-2">JTRASHED</option>
		</field>
	</fieldset>
</form>
